{
  "name": "angular-webapp",
  "version": "1.0.0",
  "description": "Una pagina web Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "npx playwright test --config=e2e/playwright.config.ts",
    "e2e:ui": "npx playwright test --config=e2e/playwright.config.ts --ui",
    "e2e:headed": "npx playwright test --config=e2e/playwright.config.ts --headed",
    "e2e:debug": "npx playwright test --config=e2e/playwright.config.ts --debug",
    "e2e:auth": "npx playwright test --config=e2e/playwright.config.ts tests/auth/",
    "e2e:user": "npx playwright test --config=e2e/playwright.config.ts tests/user/",
    "e2e:visual": "npx playwright test --config=e2e/playwright.config.ts tests/visual/",
    "e2e:a11y": "npx playwright test --config=e2e/playwright.config.ts tests/accessibility/",
    "e2e:perf": "npx playwright test --config=e2e/playwright.config.ts tests/performance/",
    "e2e:update-snapshots": "npx playwright test --config=e2e/playwright.config.ts --update-snapshots",
    "e2e:report": "npx playwright show-report e2e/playwright-report",
    "e2e:install": "npx playwright install",
    "figma:sync-tokens": "node .claude/scripts/figma-sync-tokens.js",
    "figma:generate": "node .claude/scripts/figma-generate.js",
    "figma:diff": "echo 'üîç Analyzing Figma vs Codebase differences...' && echo 'Feature coming soon!'",
    "component:generate": "ng generate component",
    "docs:generate": "echo 'üìù Generating component documentation...' && echo 'Feature coming soon!'",
    "claude:setup": "echo 'üé® Claude Code + Figma Integration Setup' && npm run figma:sync-tokens",
    "images:optimize": "node scripts/optimize-images.js",
    "validate:flow": "npx ts-node --project tools/tsconfig.json tools/flow-graph-validator.ts"
  },
  "dependencies": {
